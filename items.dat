[itemparameters]
typeid=int; id for type of basic item type; equipment =1, map interactables =2, etc
type=int; type for item; weapon, helm, gloves, etc.
type_sub=int; detailed subtype for item; sword, axe, spear, etc.
nameid=string; name of item
imagedat=int; id from separate dat file detailing all 2d gfx and associated flavor text; icon, shopicon, short text, long text, etc.
voxeldat=int; id from separate dat file detailing all 3d gfx and associated visual effects; voxel model, voxel animations, particle effects cases, etc.
sounddat=int; id from separate dat file detailing sounds of various cases; onswing1, onswing2, consume, etc.
name_text=string; in-game name for display
short_text=string; short, one-line description
long_text=string; flavor/lore paragraph
armor=int; basic stat bonus from item
strength=int; basic stat bonus from item
dexterity=int; basic stat bonus from item
intelligence=int; basic stat bonus from item
luck=int; basic stat bonus from item
movement=int; basic stat bonus from item
health=int; basic stat bonus from item
durability=int; basic durability of item before converting to death_recipe_id
minLevel_req=int; minimum required level to use
maxLevel_req=int; maximum level able to use
class_req=int;list; class required to use
race_req=int;list; race required to use
rarity_req=int;list; rarity required to use
title_req=int;list; various earnable titles required to use
consumable=int; 0 non-consumable, 1 consumable
uses=int; number of uses if consumable
ally=int; targets affected; 0 hostile npc, 1 hostile npc and allies, 2 allies only
range=int; range in tiles from user
area_id=int; area attack pattern from list: (see list)
onHit_effect=int; cast on target; basic effect id's from list
onHit_level=int; strength of onHit_effect
onUse_effect=int; cast on self; basic effect id's from list
onUse_level=int; strength of onUse_effect
cast_effect=int; from separate dat file, casts spell on target
cast_spell_effect=int; from separate dat file, casts spell on self
onEquip_effect=int; list; from separate dat file, casts effect on self
onEquip_level=int; list; strength of onEquip_effect
element=int; from list; elemental type of item for damage purposes; cold, heat, holy, dark, etc.
material=int; from list; material type of item for damage purposes; silver, salt, crystal, iron, etc.
damage_base=int; minimum damage dealt by item
damage_dice=int; number of sides per damage dice
damage_num_dice=int; number of damage dice rolled
death_id=int; used when durability reaches 0; creates appropriate items by destruction of item
death_quantity=int; number of death_id items created upon destruction
value=int; monetary value

