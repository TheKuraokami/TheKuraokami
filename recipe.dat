[recipeParameters]
recipieID=string; recipe ID
skill=int; skill used to craft; from parameters table
difficulty=int; minimum skill required to craft; affects experience gain of skill
imageDat=int; 2d image data from separate dat file
voxelDat=int; 3d image data from separate dat file
soundDat=int; sound data from separate dat file
name_text=string; written name of recipe
short_text=string; short written description of recipe
long_text=string; long written descritption/lore of recipe
tool=int; tool item required to craft
tool_expense=int; durability reduction of tool per use
workstation=int; workstation item required to craft; must be within 1 tile to craft
resource=int; list/array; item(s) expended to craft
resource_quantity=int; list/array; number of item(s) expended
product=int; list/array; product(s) created when crafting
product_quantity=int; list/array; number of product(s) created

