[abilityParameters]
abilityId=string; simple ability name
imageDat=int; 2d image data from separate dat file
voxelDat=int; 3d image data from separate dat file
soundDat=int; sound data from separate dat file
name_text=string; written name of ability
short_text=string; short written description of ability
long_text=string; long written descritption/lore of ability
armor=int; armor stat bonus
strength=int; strength stat bonus
dexterity=int; dexterity stat bonus
intelligence=int; intelligence stat bonus
luck=int; luck stat bonus
movement=int; movement stat bonus
health=int; health stat bonus
baseCost=int; base ability cost in terms of mp, rp, cp, fp, etc.
duration=int; duration of non-direct damage ability effects
ally=int; targetting; 0=hostile npc, 1=hostile npc & allies, 2=allies
range=int; base range
area_id=int; from parameter table; tile layout of area of effect
onHit_effect=int; from parameter table; basic status effect applied to target hit by ability
onHit_level=int; strength of onHit_effect
onHit_chance=int; base chance of onHit_effect being applied to target
onUse_effect=int; from parameter table; basic status effect appled to user when performing ability
onUse_level=int; strength of onHit_effect
onUse_chance=int; base chance of onUse_effect being applied to user
cast_effect=int; ability cast on user when performing ability
cast_effect_chance=int; base chance of cast_effect being applied to user
cast_spell_effect=int; ability cast on target when performing ability
cast_spell_effect_chance=int; base chance of cast_spell_effect being applied to target
element=int; from parameter table; type of damage dealt by ability, e.g., ice, fire, lightning, etc.
material=int; from parameter table; material type of ability used, e.g., iron, salt, silver, etc.
damage_base=int; minimum damage dealt by ability
damage_dice=int; number of sides per damage dice
damage_num_dice=int; number of damage dice per use

